<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <packageSources>
    <add key="nuget.org" value="https://api.nuget.org/v3/index.json" />
    <add key="msiccdev_github" value="https://nuget.pkg.github.com/msiccdev/index.json" />
  </packageSources>
  
  <packageSourceMapping>
    <!-- Public packages from nuget.org -->
    <packageSource key="nuget.org">
      <package pattern="Microsoft.*" />
      <package pattern="System.*" />
      <package pattern="ModelContextProtocol*" />
      <package pattern="xunit*" />
      <package pattern="NSubstitute*" />
      <package pattern="AwesomeAssertions*" />
      <package pattern="coverlet.*" />
      <package pattern="*" />
    </packageSource>
    
    <!-- Your personal packages from GitHub -->
    <packageSource key="msiccdev_github">
      <package pattern="MSiccDev.*" />
      <!-- Add your specific package patterns here -->
    </packageSource>
  </packageSourceMapping>
</configuration>